@article{Baugh2004,
  title = {Developing History/Historicizing Development in Mexican Nuevo Cine Manifestoes around "la Crisis".},
  volume = {34},
  issn = {03603695},
  url = {http://0-search.ebscohost.com.millenium.itesm.mx/login.aspx?direct=true\&db=aph\&AN=15221776\&site=ehost-live},
  abstract = {This essay examines the function of Nuevo Cine Group manifestoes within a framework of social scientific approaches to the history of Mexican political economy. The author argues that in their call for independence, the manifestoes critique the official discourse of modernity and development that dominate studies of Mexico and the Latin American global region more broadly. These manifestoes, thus, ensure the filmmakers freedom of expression and technical innovation outside the strictures of the Mexican film industry, foster a democratizing cultural space within Mexico's cinema, and engender national and cultural identifications for Mexican cinema audiences--all seeds for the larger cultural transformative project that continues in the years to follow. As such, according to the author, examining these manifestoes in this light recasts the history of Mexican democracy, one that reimagines crisis and contests dependency. The author concludes that there is little evidence of the success (if success is measured in quantitative numbers of actual films produced outside of the state-supported studio industry and distributed domestically and internationally by the members of the Mexican New Cinema Group) of Nuevo Cine filmmakers in instigating change in Mexican cinema or, more generally, in the economy, the socio-politics, and the cultural order in 1960s Mexico.},
  timestamp = {2015-05-07 19:09:01},
  number = {2},
  journaltitle = {Film \& History (03603695)},
  shortjournal = {Film \& History (03603695)},
  author = {Baugh, Scott L.},
  date = {2004-09},
  pages = {25--37},
  keywords = {CULTURE,DEMOCRACY,DEMOCRATIZATION,ECONOMICS,MEXICO,MODERNISM (Christian theology),MOTION picture industry,MOTION pictures,NATIONAL characteristics}
}


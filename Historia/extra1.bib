@article{jones_bunuel_2006,
	title = {{BUÑUEL} {AND} {MEXICO}: {THE} {CRISIS} {OF} {NATIONAL} {CINEMA}},
	volume = {58},
	issn = {07424671},
	url = {http://millenium.itesm.mx/login?URL=?url=http://search.proquest.com/docview/212700764?accountid=41938},
	abstract = {Subjectivity and Desire, along with an increasing output of essays on previously neglected classics like El and The Criminal Life of Archibaldo de la Cruz, none of this critical work addresses the complex negotiation between Buñuel's films and the highly codified Mexican system of genres ({LiIIo} relates these popular films to Hollywood's parameters), and none of it engages the issues affecting Mexican society at the time Buñuel was filming there.  Acevedo-Muñoz follows Carlos Monsivais in pointing out that Mexican working-class comedies of the period tended to avoid any notion of class conflict by centering on sentimental leads; Buñuel's two road movies, in contrast, (one takes place in the country, the other in the city) feature collective protagonists and show the adverse effects of modernization and development on that collective.},
	pages = {49--51},
	number = {4},
	journaltitle = {Journal of Film and Video},
	eprinttype = {jstor},
	eprint = {212700764},
	author = {Jones, Julie},
	date = {2006},
	keywords = {Book reviews, Conventions, Essays, Foreign investment, {MOTION} pictures}
}


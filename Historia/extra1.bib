	issn = {03603695},
	url = {http://millenium.itesm.mx/login?URL=?url=http://search.proquest.com/docview/214055541?accountid=41938},
	abstract = {Baugh focuses on national and cultural identity formation, specifically in the independent productions of the Mexican New Cinema of the early 1960s. In an examination of the multiple conceptualizations of "independent" in the manifestoes of the group of filmmakers first claiming the Nuevo Cine in Mexico, he calls for a reconsideration of the various notions of "crisis" used to describe Mexican society and culture and the history leading up to Mexico's revolutionary 1970s "New Wave."},
	pages = {25--37},
	number = {2},
	journaltitle = {Film \& History},
	eprinttype = {jstor},
	eprint = {214055541},
	author = {Baugh, Scott L},
	date = {2004},

@thesis{tierney_myths_2002,
  title = {Myths, machismo and marginalisation: {{Strategies}} for reading cultural nationalism in the work of {{Emilio Fernandez}}},
  url = {http://0-search.proquest.com.millenium.itesm.mx/docview/276279748?accountid=41938},
  abstract = {Classical Mexican cinema, it has been proposed, represents a successful attempt by the state to establish a position of ideological dominance; to forward a paradoxically conservative vision of the post-Revolutionary Mexican nation. Emilio Fernández is conceived as the director who best embodies this aspect of classical Mexican cinema. This study makes the new argument that Fernández's oeuvre is not the coherent depiction of the institutionalized Revolution's cultural nationalism that traditional scholarship perceives it to be, but instead a fissured, contradictory cultural text. The strategies (historical, sociological, reception-based cultural studies) for reading the classical cinemas of Mexico (and Latin America) have sought to construct imaging practices that consolidate the nation building projects of the twentieth century (Jesús Martín Barbero). Taking note of current tendencies in cultural studies to look for the instabilities and incoherencies in texts that challenge hegemonic projects like "cultural nationalism," I argue that these reading protocols have resulted in the silencing of those discourses that work against nationalist projects. This dissertation proposes radical new readings of Fernández's oeuvre which reveal these gaps and silences in hegemonic cultural nationalism. It takes issue with the representations of gender, racial ideology, national identity and nationalism. Through close textual analysis, post-colonial and transnational approaches, I reveal the fissures and alterity in the texts that suggest more subversive readings of Fernández's films than those permitted by institutionalized cultural nationalism.},
  pagetotal = {211-211 p.},
  timestamp = {2016-02-10T15:09:54Z},
  langid = {english},
  institution = {{Tulane University}},
  author = {Tierney, Dolores M.},
  date = {2002},
  keywords = {0291:Language,0312:Latin American literature,0900:Motion Pictures,Communication and the arts,Cultural nationalism,Emilio,Fernandez,Gender,Language,Latin American literature,literature and linguistics,MEXICO,MOTION pictures,Race}
}

@thesis{guerrero_imagen_2005,
  location = {{Austin, Texas}},
  title = {La imagen de la revolucion y de la mujer en la novela y el cine de la revolucion mexicana},
  url = {http://0-search.proquest.com.millenium.itesm.mx/docview/305372745?accountid=41938},
  abstract = {The focus of this study is on the colonial discourse of the political society and state embedded within the Mexican novel and cinema depicting the Mexican Revolution. A critical analysis is set forth focusing on the image that is constructed of the social movement and of women within this context and these two mediums of communication. The intent of this study is to show how myths, an integral part of colonial discourse in any society, play a central role in the construction of a post-revolutionary cultural identity, which was the desired outcome of those in power or the status quo. Similarly, the intent is to deconstruct these myths to examine omissions, exaggerations, or the deformation of the reality of this social movement and the role of women in the Mexican Revolution. What this analysis reveals is that the image of the Mexican Revolution in Mexican novels primarily represents the perspective of the revolutionary bourgeoisie, the social class to which most of the novelists and consumers of novels belonged. Somewhat in contrast, and in general, Mexican cinema projected a more favorable though idealized image of the common revolutionary and of the revolution. The image of women portrayed through the novel was sparse and they rarely appeared as revolutionaries or intellectuals. The only two instances where they are represented as such is in one novel authored by a woman and another written by a Marxist. Within cinema, where women were portrayed as revolutionaries, their image can be characterized as extreme or caricature-like. The study is significant in that very few studies jointly examine the role of these two mediums as tools for colonial discourse. To my knowledge there is no study that examines the patriarchal image of this social movement or the image of the Mexican women revolutionaries in either the Mexican novel or cinema. In fact, it has only been about two decades since researchers began to examine the role of women in the Mexican Revolution from an official historical perspective.},
  pagetotal = {261},
  timestamp = {2016-02-10T15:14:04Z},
  institution = {{Graduate School of  the University of Texas at Austin}},
  author = {Guerrero, Maria Consuelo},
  date = {2005-05},
  keywords = {0312:Latin American literature,0453:Womens studies,0900:Motion Pictures,Cine,Communication and the arts,Film,Language,Latin American literature,literature and linguistics,Mexican Revolution,MOTION pictures,Mujer,Novela,Social sciences,Womens studies}
}

@thesis{aguilar_construccion_2014-1,
  title = {La construccion de la imagen de Mexico a traves de la filmografia de Emilio ``El Indio'' Fernandez},
  url = {http://0-search.proquest.com.millenium.itesm.mx/docview/1616730850?accountid=41938},
  abstract = {Emilio Fernández es reconocido a nivel intemacional como el creador del cine indigenista de la Epoca del Cine de Oro Mexicano. En su obra, el director construyo una imagen de México basada en una ideologia personal. En éste anâlisis se aborda cómo los elementos naturales y humanos de su filmografia, convergen bajo la configuracion de conceptos abstractos como el nacionalismo, indigenismo, ideologia politica y el sincretismo. Éste análisis está cimentado en la examinación de filmes, documentales y textos relacionados con el director. El estudio asocia la obra de "El Indio" con el pensamiento prehispánico y algunas filosofias de pensadores del siglo XX. De las conclusiones se obtiene que Fernández construye una imagen utopica del pais, conformada por la fusion de raices prehispánicas y criollas. El cineasta también incorpora estereotipos definitorios de las relaciones entre sexos y las clases sociales. Sin embargo, los cambios acaecidos en la sociedad mexicana precipitaron el colapso de la imagen foijada a través de sus producciones.},
  pagetotal = {165},
  timestamp = {2016-02-10T15:18:19Z},
  langid = {spanish},
  institution = {{Stephen F. Austin State University}},
  author = {Aguilar, Zaida Fabiola},
  date = {2014-05},
  keywords = {0435:Cinematography,0550:Latin American Studies,0737:Hispanic American studies,Cinematography,Communication and the arts,Emilio Fernandez,Hispanic American studies,Identidad Mexicans,Imagen Nacional Mexicana,Latin American Studies,Social sciences}
}

@article{pablos_escuela_1998-1,
  title = {Una escuela en celuloide. {{El}} cine de {{Emilio}} "{{Indio}}" {{Fernández}} o la obsesión por la educación},
  volume = {48},
  issn = {01850172},
  abstract = {Emilio "Indio" Fern\&\#xe1;ndez es el director cinematogr\&\#xe1;fico que mejor expresa las ideas del nacionalismo, y especialmente durante las d\&\#xe9;cadas de 1940 y 1950 propone en im\&\#xe1;genes f\&\#xed;lmicas un proyecto de pa\&\#xed;s que tiene en la educaci\&\#xf3;n una de sus piezas clave. La influencia de los educadores de los a\&\#xf1;os veinte y treinta, particularmente de Jos\&\#xe9; Vasconcelos, es palpable en su cine. En este trabajo se examinan las propuestas del "Indio" en torno al tema de la educaci\&\#xf3;n y su manera de plasmarlas en celuloide, analizando algunos de sus filmes. Sus pel\&\#xed;culas ayudan a entender los s\&\#xed;mbolos culturales y las ideas acerca del nacionalismo y de la educaci\&\#xf3;n en la primera mitad del siglo XX. /// Emilio "Indio" Fern\&\#xe1;ndez is the film director who has best expressed the ideas of nationalism, and who proposed, during the forties and fifties and through cinematographic images, a project of nation where education was the key element. The influence of the 20's and 30's educators, specially that of Jos\&\#xe9; Vasconcelos, is clearly felt in his movies. By reviewing some of his films, this work analyzes "el Indio's" educational proposals and his ways of expressing them on celluloid. His films help to understand the cultural symbols and ideas about nationalism and education during the first half of the twentieth century.},
  timestamp = {2016-02-10T15:19:56Z},
  number = {2},
  journaltitle = {Historia Mexicana},
  shortjournal = {Historia Mexicana},
  author = {Pablos, Julia Tuñón},
  date = {1998},
  pages = {437--470}
}

